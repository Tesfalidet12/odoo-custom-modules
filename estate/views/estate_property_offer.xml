<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="estate_offer_action" model="ir.actions.act_window">
        <field name="name">Offer</field>
        <field name="res_model">property.offer</field>
        <field name="view_mode">list,form</field>

    </record>
    <record id="estate_offer_tree_view" model="ir.ui.view">
        <field name="name">estate.offer.tree.view</field>
        <field name="model">property.offer</field>
        <field name="arch" type="xml">
            <list>
                <field name="price"/>
            </list>
        </field>
    </record>

</odoo>